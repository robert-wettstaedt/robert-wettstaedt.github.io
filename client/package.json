{
  "name": "r-wettstaedt",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "~2.2.0",
    "normalize.css": "~3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.1",
    "babel-core": "~6.4.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.3.13",
    "babelify": "~7.2.0",
    "browser-sync": "~2.11.1",
    "browserify": "~13.0.0",
    "del": "~2.2.0",
    "envify": "~3.4.0",
    "foldero": "~0.1.1",
    "glob": "~6.0.4",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.3.0",
    "gulp-cssnano": "~2.1.0",
    "gulp-data": "~1.2.1",
    "gulp-eslint": "~2.0.0",
    "gulp-htmlmin": "~1.3.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.4.0",
    "gulp-jade": "~1.1.0",
    "gulp-load-plugins": "~1.2.0",
    "gulp-newer": "~1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.1.1",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.1",
    "gulp-util": "~3.0.7",
    "imagemin-jpegtran": "~4.2.0",
    "imagemin-pngquant": "~4.2.0",
    "imagemin-svgo": "~4.2.0",
    "jade": "~1.11.0",
    "js-yaml": "~3.5.2",
    "lodash": "~4.0.0",
    "minimist": "~1.2.0",
    "rimraf": "~2.5.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.7.0 ",
    "wrench": "~1.5.8"
  },
  "scripts": {
    "start": "rm -rf tmp && gulp serve",
    "build": "rm -rf build && gulp --production"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "base**.js",
      "css": [
        "main**.{sass,scss}",
        "base**.{sass,scss}"
      ]
    }
  }
}
